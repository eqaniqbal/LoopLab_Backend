{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "UserProfile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_profile"
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "admin",
        "student",
        "teacher"
      ]
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "userstatus": {
      "type": "enumeration",
      "enum": [
        "active",
        "suspended",
        "pending"
      ]
    },
    "leaderboardPoints": {
      "type": "integer"
    },
    "badges": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::badge.badge",
      "mappedBy": "user_profiles"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "user_profile"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "user_profile"
    },
    "eventsregisteration": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "registerations"
    },
    "announcements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::announcement.announcement",
      "mappedBy": "user_profile"
    },
    "chat_threads": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat-thread.chat-thread",
      "mappedBy": "user_profiles"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "user_profile"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "user_profile"
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "name": {
      "type": "string"
    }
  }
}
