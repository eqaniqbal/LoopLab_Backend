{
  "kind": "collectionType",
  "collectionName": "user_tables",
  "info": {
    "singularName": "user-table",
    "pluralName": "user-tables",
    "displayName": "UserTable"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_name": {
      "type": "string"
    },
    "user_password": {
      "type": "string"
    },
    "user_email": {
      "type": "email"
    },
    "user_roles": {
      "type": "enumeration",
      "enum": [
        "teacher",
        "student",
        "admin"
      ]
    },
    "auth_provider": {
      "type": "enumeration",
      "enum": [
        "google",
        "facebook"
      ]
    },
    "profile_photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "user_table"
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "user_table"
    },
    "live_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::live-session.live-session",
      "mappedBy": "user_table"
    },
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event"
    },
    "event_registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-registration.event-registration",
      "mappedBy": "user_table"
    },
    "announcements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::announcement.announcement",
      "mappedBy": "user_table"
    },
    "chats": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "user_table"
    },
    "user_badges": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::badge.badge",
      "mappedBy": "user_table"
    },
    "chatss": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "user_tables"
    },
    "leader_boards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::leader-board.leader-board",
      "mappedBy": "user_table"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "user_table"
    }
  }
}
