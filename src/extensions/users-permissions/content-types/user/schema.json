{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "name": {
      "type": "string"
    },
    "auth_provider": {
      "type": "enumeration",
      "enum": [
        "Email",
        "Google",
        "Facebook"
      ]
    },
    "Roles": {
      "type": "enumeration",
      "enum": [
        "Teacher",
        "Student ",
        "Admin"
      ]
    },
    "User_status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Suspended ",
        "Deleted"
      ]
    },
    "profile_photo_url": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "users_permissions_user"
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "users_permissions_user"
    },
    "live_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::live-session.live-session",
      "mappedBy": "users_permissions_user"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "users_permissions_user"
    },
    "event_registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-registration.event-registration",
      "mappedBy": "users_permissions_user"
    },
    "announcements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::announcement.announcement",
      "mappedBy": "users_permissions_user"
    },
    "chats": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "users_permissions_user"
    },
    "chats_sender": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "user_sender"
    },
    "badges": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::badge.badge",
      "mappedBy": "users_permissions_user"
    },
    "leader_boards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::leader-board.leader-board",
      "mappedBy": "users_permissions_user"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "users_permissions_user"
    }
  }
}
